<div class="tab-pane fade" id="add-fachieve" role="tabpanel" aria-labelledby="profile-tab">
    <div class="card shadow mb-4 mb-2">
        <div class="card-body">
            <form action="/admin/fachieve" method="POST">
                <div class="modal-body">
                    <div class="form-group">
                        <label for="name">Fakultas</label>
                        <select class="form-control" name="facultyId">
                            <option value="" disabled selected>Fakultas</option>
                            <% for(i = 0; i < faculty.length; i++) { %>
                                <option value="<%= faculty[i].id %>"><%= faculty[i].faculty %></option>
                            <% } %>
                        </select>
                    </div>
                    <div class="form-group">
                        <label for="name">Jumlah Mahasiswa</label>
                        <input type="number" class="form-control" name="studQty" placeholder="Enter Qty Mahasiswa" required>
                    </div>
                    <div class="form-group">
                        <label for="name">Jumlah Prestasi</label>
                        <input type="number" class="form-control" name="medalsQty" placeholder="Enter Qty Medals" required>
                    </div>
                    <div class="form-group">
                        <label for="name">Jumlah Paten</label>
                        <input type="number" class="form-control" name="patenQty" placeholder="Enter Qty Paten" required>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                    <button type="submit" class="btn btn-primary">Add</button>
                </div>
            </form>
        </div>
    </div>
</div>

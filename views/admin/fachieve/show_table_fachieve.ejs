  <div class="tab-pane fade show active" id="show-fachieve" role="tabpanel" aria-labelledby="home-tab">
    <div class="card shadow mb-4">
      <div class="card-body mt-2">
        <div class="table-responsive">
          <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
            <thead>
              <tr>
                <th>No</th>
                <th>Name Fakultas</th>
                <th>Jumlah Mahasiswa</th>
                <th>Jumlah Prestasi</th>
                <th>Jumlah Paten</th>
                <th>Action</th>
              </tr>
            </thead>
            <tbody>
              <% for(let i = 0; i < fachieve.length; i++) { %>
                <tr>
                  <td><%= i+1 %></td>
                  <td><%= fachieve[i].facultyId.faculty %></td>
                  <td><%= fachieve[i].studQty %></td>
                  <td><%= fachieve[i].medalsQty %></td>
                  <td><%= fachieve[i].patenQty %></td>
                  <td>
                    <form action="/admin/fachieve/<%= fachieve[i].id %>/delete?_method=DELETE" method="POST">
                      <a href="/admin/fachieve/<%= fachieve[i].id %>" class="btn btn-warning btn-circle btn-sm"><i class="fas fa-edit"></i></a>
                      <button type="submit" class="btn btn-danger btn-circle btn-sm"><i class="fas fa-trash"></i></button>
                  </form>
                  </td>
                </tr>
              <% } %>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
  
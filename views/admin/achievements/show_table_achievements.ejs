<div class="tab-pane fade show active" id="show-achievements" role="tabpanel" aria-labelledby="home-tab">
  <div class="card shadow mb-4">
    <div class="card-header py-3">
      <h6 class="m-0 font-weight-bold text-primary">Table Jurusan</h6>
    </div>
    <div class="card-body">
      <div class="table-responsive">
        <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
          <thead>
            <tr>
              <th>Nama</th>
              <th>NPM</th>
              <th>Kepersertaan</th>
              <th>Event</th>
              <th>Peringkat</th>
              <th>Kategori</th>
              <th>Jenis</th>
              <th>Karya</th>
              <th>Jumlah Negara</th>
              <th>Jumlah Universitas</th>
              <th>Tanggal Awal</th>
              <th>Tanggal Akhir</th>
              <th>Action</th>
            </tr>
          </thead>
          <tbody>
            <% for(let i = 0; i < prestasi.length; i++) { %>
              <tr>
                <td><%= prestasi[i].studentId.name %></td>
                <td><%= prestasi[i].studentId.npm %></td>
                <td><%= prestasi[i].kepesertaan %></td>
                <td><%= prestasi[i].event %></td>
                <td><%= prestasi[i].peringkat %></td>
                <td><%= prestasi[i].category %></td>
                <td><%= prestasi[i].type %></td>
                <td><%= prestasi[i].creation %></td>
                <td><%= prestasi[i].countryQty %></td>
                <td><%= prestasi[i].uniQty %></td>
                <td><%= prestasi[i].startDate.toLocaleString('default', { month: 'long' }) %> <%= prestasi[i].startDate.getUTCFullYear() %> </td>
                <td><%= prestasi[i].endDate.toLocaleString('default', { month: 'long' }) %> <%= prestasi[i].endDate.getUTCFullYear() %> </td>
                <td>
                  <form action="/admin/achievements/<%= prestasi[i].id %>/delete?_method=DELETE" method="POST">
                    <a href="/admin/achievements/<%= prestasi[i].id %>" class="btn btn-warning btn-circle btn-sm"><i class="fas fa-edit"></i></a>
                    <button type="submit" class="btn btn-danger btn-circle btn-sm"><i class="fas fa-trash"></i></button>
                </form>
                </td>
              </tr>
            <% } %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>

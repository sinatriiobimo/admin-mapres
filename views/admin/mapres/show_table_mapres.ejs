<div class="tab-pane fade show active" id="show-mapres" role="tabpanel" aria-labelledby="home-tab">
  <div class="card shadow mb-4">
    <div class="card-header py-3">
      <h6 class="m-0 font-weight-bold text-primary">Table Jurusan</h6>
    </div>
    <div class="card-body">
      <div class="table-responsive">
        <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
          <thead>
            <tr>
              <th>No</th>
              <th>Nama</th>
              <th>Fakultas</th>
              <th>Best Achievements</th>
              <th>Tips</th>
              <th>Action</th>
            </tr>
          </thead>
          <tbody>
            <% for(let i = 0; i < mapres.length; i++) { %>
              <tr>
                <td><%= i+1 %></td>
                <td><%= mapres[i].studentId.name %></td>
                <td><%= mapres[i].facultyId.faculty %></td>
                <td><%= mapres[i].label %></td>
                <td><%= mapres[i].tips %></td>
                <td>
                  <form action="/admin/mapres/<%= mapres[i].id %>/delete?_method=DELETE" method="POST">
                    <a href="/admin/mapres/<%= mapres[i].id %>" class="btn btn-warning btn-circle btn-sm"><i class="fas fa-edit"></i></a>
                    <button type="submit" class="btn btn-danger btn-circle btn-sm"><i class="fas fa-trash"></i></button>
                </form>
                </td>
              </tr>
            <% } %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>

<div class="tab-pane fade" id="add-mapres" role="tabpanel" aria-labelledby="profile-tab">
    <div class="card shadow mb-4 mb-2">
        <div class="card-body">
            <form action="/admin/mapres" method="POST">
                <div class="form-group">
                    <div class="form-group">
                        <label for="name">Fakultas</label>
                        <select class="form-control" name="facultyId">
                            <option value="" disabled selected>Fakultas</option>
                            <% for(i = 0; i < faculty.length; i++) { %>
                                <option value="<%= faculty[i].id %>"><%= faculty[i].faculty %></option>
                                <% } %>
                            </select>
                        </div>
                        <div class="form-group">
                            <label for="name">Nama Mahasiswa</label>
                            <select class="form-control" name="studentId">
                                <option value="" disabled selected>Nama Mahasiswa</option>
                                <% for(i = 0; i < students.length; i++) { %>
                                    <option value="<%= students[i].id %>"><%= students[i].name %> - <%= students[i].majorId.name %></option>
                                    <% } %>
                                </select>
                            </div>
                            <div class="form-group">
                                <label for="name">Best Achievements</label>
                                <input type="text" class="form-control" name="label" placeholder="Enter Best Achievements" required>
                            </div>
                            <div class="form-group">
                                <label for="name">Tips</label>
                                <textarea class="form-control" name="tips" required></textarea>
                            </div>
                            <button type="submit" class="btn btn-primary">Add</button>
                        </form>
                    </div>
                </div>
            </div>